{
  "name": "@ironblocks/venn-dapp-sdk",
  "version": "0.8.0",
  "description": "SDK for interactions with Venn Network",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "git@github.com:ironblocks/venn-dapp-sdk.git",
  "author": "Mark <mwoyshvillo@gmail.com>",
  "license": "MIT",
  "keywords": [
    "ironblocks",
    "Venn Network",
    "SDK",
    "web3"
  ],
  "gitHooks": {
    "pre-commit": "yarn ts-check && yarn lint && yarn test"
  },
  "scripts": {
    "build": "tsup",
    "ts-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest --silent=false",
    "publish": "npm pack && npm publish && rm -rf *.tgz",
    "publish:local": "npm link",
    "generate-abi-types": "typechain --target=ethers-v6 'src/abi/**/*.json' --out-dir src/types/contracts"
  },
  "devDependencies": {
    "@typechain/ethers-v6": "^0.5.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.4",
    "typechain": "^8.3.2",
    "typescript": "^5.5.4",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "axios": "^1.7.3",
    "ethers": "^6.13.2"
  }
}
